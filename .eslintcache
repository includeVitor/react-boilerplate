        return acc\r\n        },{})\r\n\r\n        setErrors(errorMessages)\r\n\r\n        //forgot action\r\n        \r\n    }\r\n\r\n    const handleChange = (e: any) => {\r\n        const input = e.target\r\n        setEmail(input.value)\r\n        setErrors({...errors, [input.name] : input.validationMessage})\r\n    }\r\n\r\n    const hasError = (field: string) => !!errors[field]\r\n\r\n    const getError = (field: string) => errors[field]   \r\n\r\n    return (  \r\n        <>  \r\n            <SmartDataPaper padding={68} border={true} background={true}>\r\n                <form noValidate onSubmit={handleSubmit}>\r\n                    <Grid container>    \r\n                        <Grid item lg={12}>\r\n                            <TitleRecovery variant=\"h5\">\r\n                                Recuperar senha                          \r\n                            </TitleRecovery>\r\n                        </Grid>\r\n\r\n                        <Grid item lg={12}>\r\n                            <TextField\r\n                                type=\"email\"\r\n                                value={email}\r\n                                name=\"email\"\r\n                                onChange={handleChange}\r\n                                margin=\"normal\"\r\n                                label=\"Digite seu e-mail\" \r\n                                variant=\"outlined\"\r\n                                autoComplete=\"email\"\r\n                                helperText={getError('email')}\r\n                                error={hasError('email')}\r\n                                autoFocus\r\n                                fullWidth\r\n                                required\r\n                            />  \r\n                        </Grid>\r\n\r\n                        <Grid item lg={12}>\r\n                            <ForgotButton\r\n                                type=\"submit\"\r\n                                variant=\"contained\"\r\n                                color=\"primary\"\r\n                                size=\"medium\"\r\n                                fullWidth\r\n                            >\r\n                                Recuperar\r\n                            </ForgotButton>\r\n                        </Grid>\r\n\r\n                    </Grid>\r\n                </form>\r\n            </SmartDataPaper>\r\n\r\n            <SmartDataPaper>\r\n\r\n                <SecurityIcon/>\r\n\r\n                <TitleMessage variant=\"h4\" >\r\n                    Volte a ter o controle de seus projetos\r\n                </TitleMessage>\r\n\r\n                <Grid item lg={12}>\r\n                    <TitleBack>\r\n                        <Link href={LOGIN} onClick={handleRedirectLogin}>\r\n                            Voltar\r\n                        </Link>\r\n                    </TitleBack>\r\n                </Grid>\r\n            </SmartDataPaper>\r\n        </>\r\n    )\r\n}\r\n \r\nexport default ForgetPage","P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\auth\\Signup\\index.tsx",[],"P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\dashboard\\listItems\\index.tsx",[],"P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\auth\\Login\\index.tsx",[],"P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\auth\\Forgot\\styles.ts",[],"P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\auth\\Login\\styles.ts",[],"P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\dashboard\\listItems\\mainList.tsx",[],"P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\_layouts\\auth\\styles.ts",[],"P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\_layouts\\auth\\index.tsx",[],"P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\auth\\Signup\\styles.ts",[],"P:\\Personal_Or_Old\\react\\reactjs2.0\\src\\pages\\_layouts\\app\\styles.ts",[],{"ruleId":"128","replacedBy":"129"},{"ruleId":"130","replacedBy":"131"},{"ruleId":"128","replacedBy":"132"},{"ruleId":"130","replacedBy":"133